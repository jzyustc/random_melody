<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE score-partwise PUBLIC "-//Recordare//DTD MusicXML 3.1 Partwise//EN" "http://www.musicxml.org/dtds/partwise.dtd">
<score-partwise version="3.1">       <!-- 根节点: score-partwise -->
  <part-list>                        <!-- 行(分谱)列表 -->
    <score-part id="P1">
      <part-name>Piano</part-name>   <!-- 乐器名称 -->
    </score-part>
  </part-list>
  <part id="P1">


    <measure number="1">             <!-- 第 measure_no 小节 -->

      <attributes>                   <!-- 小节属性 -->
        <divisions>4</divisions>   <!-- 设定用 divisions 表示一个四分音符的长度 -->
        <key>
          <fifths>0</fifths>         <!-- C大调(没有升降号) -->
        </key>
        <time>
          <beats>4</beats>
          <beat-type>4</beat-type>   <!-- time_num/time_divide 拍 -->
        </time>
        <clef>
          <sign>G</sign>
          <line>2</line>             <!-- 使用高音谱号(G谱号) -->
        </clef>
      </attributes>

      <direction placement="above">
        <direction-type>
          <metronome parentheses="no">
            <beat-unit>quarter</beat-unit>
            <per-minute>140</per-minute>            <!-- bpm -->
          </metronome>
        </direction-type>
        <sound tempo="140"/>
      </direction>

      <note>

        
        <rest/>

        <duration>16</duration>         <!-- 持续时长 -->
        
        <voice>1</voice>
        <type>whole</type>



        

      </note>



    </measure>

    <measure number="2">             <!-- 第 measure_no 小节 -->

      

      <note>

        <pitch>
          <step>A</step>           <!-- 音高 -->

          

          <octave>4</octave>     <!-- 八度 -->
        </pitch>

        <duration>4</duration>         <!-- 持续时长 -->
        
        <voice>1</voice>
        <type>quarter</type>



        <lyric number="2">
          <syllabic>single</syllabic>
          <text>グー</text>                  <!-- 歌词 -->
        </lyric>

      </note>

      <note>

        <pitch>
          <step>C</step>           <!-- 音高 -->

          

          <octave>5</octave>     <!-- 八度 -->
        </pitch>

        <duration>4</duration>         <!-- 持续时长 -->
        
        <voice>1</voice>
        <type>quarter</type>



        <lyric number="1">
          <syllabic>single</syllabic>
          <text>あ</text>                  <!-- 歌词 -->
        </lyric>

      </note>

      <note>

        <pitch>
          <step>C</step>           <!-- 音高 -->

          

          <octave>5</octave>     <!-- 八度 -->
        </pitch>

        <duration>4</duration>         <!-- 持续时长 -->
        
        <voice>1</voice>
        <type>quarter</type>



        <lyric number="2">
          <syllabic>single</syllabic>
          <text>グー</text>                  <!-- 歌词 -->
        </lyric>

      </note>

      <note>

        <pitch>
          <step>C</step>           <!-- 音高 -->

          

          <octave>5</octave>     <!-- 八度 -->
        </pitch>

        <duration>4</duration>         <!-- 持续时长 -->
        
        <voice>1</voice>
        <type>quarter</type>



        <lyric number="1">
          <syllabic>single</syllabic>
          <text>あ</text>                  <!-- 歌词 -->
        </lyric>

      </note>



    </measure>

    <measure number="3">             <!-- 第 measure_no 小节 -->

      

      <note>

        <pitch>
          <step>G</step>           <!-- 音高 -->

          

          <octave>5</octave>     <!-- 八度 -->
        </pitch>

        <duration>4</duration>         <!-- 持续时长 -->
        
        <voice>1</voice>
        <type>quarter</type>



        <lyric number="2">
          <syllabic>single</syllabic>
          <text>グー</text>                  <!-- 歌词 -->
        </lyric>

      </note>

      <note>

        <pitch>
          <step>A</step>           <!-- 音高 -->

          

          <octave>5</octave>     <!-- 八度 -->
        </pitch>

        <duration>4</duration>         <!-- 持续时长 -->
        <tie type="start"/>
        <voice>1</voice>
        <type>quarter</type>

        <notations>
           <tied type="start"/>           <!-- 延音 start/stop -->
        </notations>


        <lyric number="1">
          <syllabic>single</syllabic>
          <text>あ</text>                  <!-- 歌词 -->
        </lyric>

      </note>

      <note>

        <pitch>
          <step>A</step>           <!-- 音高 -->

          

          <octave>5</octave>     <!-- 八度 -->
        </pitch>

        <duration>1</duration>         <!-- 持续时长 -->
        <tie type="stop"/>
        <voice>1</voice>
        <type>16th</type>

        <notations>
           <tied type="stop"/>           <!-- 延音 start/stop -->
        </notations>


        

      </note>

      <note>

        <pitch>
          <step>C</step>           <!-- 音高 -->

          

          <octave>6</octave>     <!-- 八度 -->
        </pitch>

        <duration>4</duration>         <!-- 持续时长 -->
        
        <voice>1</voice>
        <type>quarter</type>



        <lyric number="2">
          <syllabic>single</syllabic>
          <text>グー</text>                  <!-- 歌词 -->
        </lyric>

      </note>

      <note>

        <pitch>
          <step>C</step>           <!-- 音高 -->

          

          <octave>6</octave>     <!-- 八度 -->
        </pitch>

        <duration>2</duration>         <!-- 持续时长 -->
        <tie type="start"/>
        <voice>1</voice>
        <type>eighth</type>

        <notations>
           <tied type="start"/>           <!-- 延音 start/stop -->
        </notations>


        <lyric number="1">
          <syllabic>single</syllabic>
          <text>あ</text>                  <!-- 歌词 -->
        </lyric>

      </note>

      <note>

        <pitch>
          <step>C</step>           <!-- 音高 -->

          

          <octave>6</octave>     <!-- 八度 -->
        </pitch>

        <duration>1</duration>         <!-- 持续时长 -->
        <tie type="stop"/>
        <voice>1</voice>
        <type>16th</type>

        <notations>
           <tied type="stop"/>           <!-- 延音 start/stop -->
        </notations>


        

      </note>



    </measure>

    <measure number="4">             <!-- 第 measure_no 小节 -->

      

      <note>

        
        <rest/>

        <duration>16</duration>         <!-- 持续时长 -->
        
        <voice>1</voice>
        <type>whole</type>



        

      </note>



    </measure>


  </part>
</score-partwise>